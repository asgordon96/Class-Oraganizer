<div class"container">
  <div class="row">
    <div class="col-md-4 col-md-offset-4">
      <h1>Sign In</h1>

      <%= form_tag('/sessions/create', method:'post') do %>
        <div class="form-group">
          <%= label_tag :email %></td>
          <%= text_field_tag :email, "", {class: 'form-control'}  %></td>
        </div>
        <div class="form-group">
          <%= label_tag :password  %></td>
          <%= password_field_tag :password, "", {class: 'form-control'}  %></td>
        </div>
        <%= submit_tag 'Sign In', {class: 'btn btn-primary'} %>
      <% end %>
    </div>
  </div>
</div>